<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="fr.esigelec.garageHibernate">
<class name="Voiture" table="voiture">
<id name="id" column="ID">
<generator class="native"/>
</id>
<property name="immatriculation" column="immatriculation" type="string"/>
<property name="modele" column="modele" type="string"/>
<property name="proprietaire" column="proprietaire" type="string"/>
<property name="anneeConstruction" column="anneeConstruction" type="integer"/>
<property name="km" column="km" type="integer"/>
</class>
</hibernate-mapping>